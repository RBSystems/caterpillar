{
  "caterpillars": [
    {
      "id": "CoreStateTime",
      "type": "core-state-time-machine",
      "index": "av-delta-events*",
      "interval": "0 * * * * *",
      "time-field": "timestamp",
      "max-interval": "12h",
      "query": {
        "query": {
          "bool": {
            "must": [
              {"match": {"event-tags": "core-state"}},
              {"match": {"affected-room.buildingID": "ITB"}}
            ]
          }
        }
      },
      "type-config": {
        "output-index": "caterpillar-test"
      }
    }
  ],
  "store-location": "/home/sir/work/badgertest/caterpillar"
}
