{
  "caterpillars": [
    {
      "id": "joe_test",
      "type": "joe_test",
      "index": "av-all-events*",
      "interval": "0 * * * * *",
      "time-field": "timestamp",
      "max-interval": "",
      "query": {
        "query": {
          "bool": {
            "must": [{"match": {"generating-system": "ITB-1101-CP1"}}]
          }
        }
      },
      "type-config": {
        "output-index": "caterpillar-test"
      }
    },
    {
      "id": "CoreStateTime",
      "type": "core-state-time",
      "index": "av-delta-events*",
      "interval": "0 * * * * *",
      "time-field": "timestamp",
      "max-interval": "2h",
      "query": {
        "query": {
          "bool": {
            "must": [
              {"match": {"event-tags": "core-state"}},
              {"match": {"target-device.deviceID": "ITB-1101-D1"}}
            ]
          }
        }
      },
      "type-config": {
        "output-index": "caterpillar-test"
      }
    }
  ],
  "store-location": "/home/sir/work/badgertest/caterpillar"
}
